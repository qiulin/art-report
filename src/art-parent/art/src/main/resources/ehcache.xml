<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:noNamespaceSchemaLocation="ehcache.xsd"
		 updateCheck="false" monitoring="autodetect"
		 dynamicConfig="true"
		 eternal="false" overflowToDisk="false" maxEntriesLocalHeap="100"
		 timeToIdleSeconds="180" timeToLiveSeconds="300">
	
	<diskStore path="java.io.tmpdir" />
	
	<defaultCache eternal="false" maxEntriesLocalHeap="100" overflowToDisk="false" />
	
	<cache name="reports"
		   timeToIdleSeconds="1200" timeToLiveSeconds="1800"
		   eternal="false" maxEntriesLocalHeap="10" />
	
	<cache name="users"
		   eternal="true" maxEntriesLocalHeap="10" />
	
	<cache name="userGroups"
		   eternal="true" maxEntriesLocalHeap="10" />
	
	<cache name="datasources"
		   eternal="true" maxEntriesLocalHeap="10" />
	
	<cache name="reportGroups"
		   eternal="true" maxEntriesLocalHeap="10" />
	
	<cache name="schedules"
		   eternal="true" maxEntriesLocalHeap="10" />
	
	<cache name="jobs"
		   eternal="true" maxEntriesLocalHeap="10" />
	
	<cache name="rules"
		   eternal="true" maxEntriesLocalHeap="10" />
	
	<cache name="parameters"
		   eternal="true" maxEntriesLocalHeap="10" />
	
	<cache name="encryptors"
		   eternal="true" maxEntriesLocalHeap="10" />
	
	<cache name="holidays"
		   eternal="true" maxEntriesLocalHeap="10" />
	
	<cache name="destinations"
		   eternal="true" maxEntriesLocalHeap="10" />
	
	<cache name="smtpServers"
		   eternal="true" maxEntriesLocalHeap="10" />
	
	<cache name="permissions"
		   eternal="true" maxEntriesLocalHeap="10" />
	
	<cache name="roles"
		   eternal="true" maxEntriesLocalHeap="10" />
	
	<cache name="drilldowns"
		   eternal="true" maxEntriesLocalHeap="10" />
	
</ehcache>